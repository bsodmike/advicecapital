{"ts":1347956288159,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"require 'clockwork'\nrequire File.expand_path('../config/boot', File.dirname(__FILE__))\nrequire File.expand_path('../config/environment', File.dirname(__FILE__))\n\n\nmodule Clockwork\n  stock_data = Ystock.find([\n    'MAERSK-A.CO', # A.P. Møller Mærsk A\n    'MAERSK-B.CO', # A.P. Møller Mærsk B\n    'CARL-B.CO',   # Carlsberg\n    'CHR.CO',      # Chr. Hansen Holding\n    'COLO-B.CO',   # Coloplast\n    'DANSKE.CO',   # Danske Bank\n    'DSV.CO',      # DSV\n    'FLS.CO',      # FLSmidth & Co\n    'GN.CO',       # GN Store Nord\n    'LUN.CO',      # H. Lundbeck\n    'NKT.CO',      # NKT Holding\n    'NDA-DKK.CO',  # Nordea Bank\n    'NOVO-B.CO',   # Novo Nordisk\n    'NZYM-B.CO',   # Novozymes\n    'PNDORA.CO',   # Pandora\n    'TDC.CO',      # TDC\n    'TOP.CO',      # Topdanmark\n    'TRYG.CO',     # Tryg\n    'VWS.CO',      # Vestas\n    'WDH.CO'       # William Demant Holding\n  ])\n\n  every(5.minutes, 'stock_data.update') do\n    \n    stock_data.each do |key, hash|\n      StockData.where(:symbol => hash[:symbol]).last.update_attributes(:price => hash[:price], :change => hash[:change],:volume => hash[:volume])\n    end  \n  \n    puts \"Rake[:stock_info_update] => StockData updated at #{Time.now.strftime(\"%D @ %H%M %z\")}\"\n  end\nend\n\n\n"]],"start1":0,"start2":0,"length1":0,"length2":1227}]],"length":1227}
